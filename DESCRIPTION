Package: mbquartR
Title: Finding Manitoba Quarter Sections
Version: 0.1.0
Authors@R: c(
    person("Alex", "Koiter", , "koitera@brandonu.ca", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0002-9355-9561")),
